void Display_Graphic_16x16(unsigned char page,unsigned char column,unsigned char *dp)
{
	unsigned char i,j;
	for(j=0;j<2;j++)
	{
		Lcd_Address(page+j,column);
		for (i=0;i<16;i++)
		{	
			send_dat(*dp);
			dp++;
		}
	}
}

