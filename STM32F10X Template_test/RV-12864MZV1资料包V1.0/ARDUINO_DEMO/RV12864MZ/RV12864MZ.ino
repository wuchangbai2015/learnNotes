// the setup routine runs once when you press reset:
void setup() {                
  // initialize the digital pin as an output.
  pinMode(bl, OUTPUT);
  pinMode(rst, OUTPUT);
  pinMode(cs , OUTPUT);
  pinMode(dc , OUTPUT);
  pinMode(sdi, OUTPUT);
  pinMode(sck, OUTPUT);  
}

void LCDShiftWrite(unsigned char dat)   
{    
  unsigned char i;                                      
  unsigned char Series,Temp; 
	                                         
  Series = dat;   
  for(i=0; i<8; i++)   
  {   
    digitalWrite(sck, LOW);                              
    Temp=Series & 0x80;                    
    if(Temp)                              
    {   
      digitalWrite(sdi, HIGH);                 
    }   
    else    
    {   
      digitalWrite(sdi, LOW);                   
    } 
    digitalWrite(sck, HIGH);                            
    Series = Series << 1;               
  }   
}   


void send_cmd(unsigned char cmd)
{
  digitalWrite(cs, LOW);
  digitalWrite(dc, LOW);
  LCDShiftWrite(cmd);
  digitalWrite(cs, HIGH);
}

void send_dat(unsigned char dat)
{
  digitalWrite(cs, LOW);
  digitalWrite(dc, HIGH);
  LCDShiftWrite(dat);
  digitalWrite(cs, HIGH);
}

void LCDInit(void) 
{ 
  digitalWrite(rst, LOW);
  delay(10);
  digitalWrite(rst, HIGH);
  delay(10);
	
  send_cmd(0xe2);	
  delay(5);
  send_cmd(0xa0);  
  send_cmd(0xc8); 
  send_cmd(0xa2);
  send_cmd(0x2f);	
  send_cmd(0x25);	
  send_cmd(0x81); 
  send_cmd(0x19); 
  send_cmd(0x40);  
  send_cmd(0xaf); 
  cls();
  digitalWrite(bl, HIGH);
} 

void Lcd_Address(unsigned char page,unsigned char column)
{
	column=column-0x01;
	send_cmd(0xb0+page-1);  
	send_cmd(0x10+(column>>4&0x0f));
	send_cmd(column&0x0f);	
}

void cls(void)
{
  unsigned char i,j;
	for(i=0;i<9;i++)
	{
		send_cmd(0xb0+i);  
		send_cmd(0x10);
		send_cmd(0x00);
		for(j=0;j<132;j++)
		{
		 	send_dat(0x00);
		}
	}
}

