void Display_Graphic_5x8(unsigned char page,unsigned char column,unsigned char *dp)
{
    unsigned char col_cnt;
    Lcd_Address(page,column);
	for (col_cnt=0;col_cnt<5;col_cnt++)
	{	
		send_dat(*dp);
		dp++;
	}
}

void Scribing(void)
{
	unsigned char i;
	Lcd_Address(2, 1);
	for (i=0; i<128; i++)
	{
		send_dat(0x08);
	}		
}

void DisplayFont(unsigned char page, unsigned char column, unsigned char No, unsigned char typeface)
{
	unsigned char *dp;
	switch (typeface)
	{
		case FONT_5X8:
		  dp = font5x8[No];
			Display_Graphic_5x8(page, column, dp);
			break;
		case FONT_8X16:
			dp = font8x16[No];
			Display_Graphic_8x16(page, column, dp);
			break;
		case FONT_16X16:
			dp = font16x16[No];
			Display_Graphic_16x16(page, column, dp);
			break;
		case FONT_128X64:
			dp = IMG128x64;
			Display_128x64(dp);
			break;
		case SCRIBING:
			Scribing();
			break;
		default:
			break;
	}		   
}

